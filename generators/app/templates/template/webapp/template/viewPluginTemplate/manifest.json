{
    "sap.app": {
        "id": "<%= namespace %>.viewPluginTemplate",
        "type": "component",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "embeddedBy": "../"
    },

    "sap.ui5": {
        "rootView": {
            "viewName": "<%= namespace %>.viewPluginTemplate.view.PluginView",
            "type": "XML",
            "id": "viewPluginTemplate"
        },
        "resources": {
            "css": [{
                "uri": "css/style.css"
            }]
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "<%= namespace %>.viewPluginTemplate.i18n.i18n"
                }
            }
        }
    }
}